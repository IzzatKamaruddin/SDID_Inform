generator client {
    provider        = "prisma-client-js"
    previewFeatures = ["referentialIntegrity"]
}


datasource db {
    provider     = "mysql"
    url          = env("DATABASE_URL")
    relationMode = "prisma"
}



model fbbsubs {
    id String @id @default(cuid())
    state String
    declaration_quarter Int
    declaration_year Int
    household String
    total_subs Int
}

model catgory {
    id Int @id @default(cuid())
    name String
    description String
    subs subs[]
} 

model subs{
    id Int @id @default(cuid())
    state String
    declaration_quarter Int
    declaration_year Int
    total_subs Int
    category    Category? @relation(fields: [category_id], references: [id])
    category_id Int
}

model mbbsubs {
    id Int @id @default(cuid())
    state String
    service String
    service_type String
    declaration_quarter Int
    declaration_year Int
    household String
    total_subs Int
}

model mbbtraffic {
    id Int @id @default(cuid())
    declaration_quarter Int
    declaration_year Int
    traffic_in Float
}

model fbbpenrate {
    id Int @id @default(cuid())
    state String
    declaration_quarter Int
    declaration_year Int
    penrate Float
}


